Xft.dpi:       96 Xft.antialias: true
Xft.hinting:   true
Xft.rgba:      rgb
Xft.autohint:  false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

Xcursor.theme: xcursor-breeze
XcursorSize:                     0

! alternative font settings with 'terminus':
! URxvt.font:      -xos4-terminus-medium-r-normal--16-160-72-72-c-80-iso10646-1
! URxvt.bold.font: -xos4-terminus-bold-r-normal--16-160-72-72-c-80-iso10646-1
!! terminus names see end of file!

URxvt*depth:                      32
URxvt*scrollBar:                  false
URxvt*mouseWheelScrollPage:       false
URxvt*cursorBlink:                true
URxvt*saveLines:                  5000

!!! COLOR CONFIG !!!


! hyatheme

URxvt*termName: rxvt
URxvt*font:     xft:Source Code Pro for Powerline
URxvt*boldFont: xft:Source Code Pro for Powerline:style=bold

! special
URxvt*foreground:           #dadada
URxvt*background:           #2c2f33
URxvt*cursorColor:          #e18dff

! black
URxvt*color0:               #1c1c1c
URxvt*color8:               #1c1c1d

! red
URxvt*color1:               #ff6584
URxvt*color9:               #ff8dab

! green
URxvt*color2:               #84ff65
URxvt*color10:              #abff8d

! yellow
URxvt*color3:               #fff563
URxvt*color11:              #fff08d

! blue
URxvt*color4:               #65a3ff
URxvt*color12:              #8dcaff

! magenta
URxvt*color5:               #cd66f2
URxvt*color13:              #e18dff

! cyan
URxvt*color6:               #65d3ff
URxvt*color14:              #8dfaff

! white
URxvt*color7:               #dadada
URxvt*color15:              #dadafa

!!! END COLORS !!!

! Normal copy-paste keybindings without perls
URxvt.iso14755:                   false
URxvt.keysym.Shift-Control-V:     eval:paste_clipboard
URxvt.keysym.Shift-Control-C:     eval:selection_to_clipboard
!Xterm escape codes, word by word movement
URxvt.keysym.Control-Left:        \033[1;5D
URxvt.keysym.Shift-Control-Left:  \033[1;6D
URxvt.keysym.Control-Right:       \033[1;5C
URxvt.keysym.Shift-Control-Right: \033[1;6C
URxvt.keysym.Control-Up:          \033[1;5A
URxvt.keysym.Shift-Control-Up:    \033[1;6A
URxvt.keysym.Control-Down:        \033[1;5B
URxvt.keysym.Shift-Control-Down:  \033[1;6B
